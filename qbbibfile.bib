@article{Mazon:2009,
	Abstract = {The development of a data warehouse (DW) system is based on a conceptual multidimensional model, which provides a high level of abstraction in accurately and expressively describing real-world situations. Once this model is designed, the corresponding logical representation must be obtained as the basis of the implementation of the \{DW\} according to one specific technology. However, even though a good conceptual multidimensional model is designed underneath a DW, there is a semantic gap between this model and its logical representation. In particular, this gap complicates an adequate treatment of summarizability issues, which in turn may lead to erroneous results of data analysis tools. Research addressing this topic has produced only partial solutions, and individual terminology used by different parties hinders further progress. Consequently, based on a unifying vocabulary, this survey sheds light on (i) the weak and strong points of current approaches for modeling complex multidimensional structures that reflect real-world situations in a conceptual multidimensional model and (ii) existing mechanisms to avoid summarizability problems when conceptual multidimensional models are being implemented. },
	Author = {Jose-Norberto Maz{\'o}n and Jens Lechtenb{\"o}rger and Juan Trujillo},
	Date-Added = {2014-09-15 14:01:32 +0000},
	Date-Modified = {2014-09-16 07:59:48 +0000},
	Doi = {10.1016/j.datak.2009.07.010},
	Issn = {0169-023X},
	Journal = {Data \& Knowledge Engineering},
	Keywords = {Data analysis},
	Note = {Including Special Section: 21st \{IEEE\} International Symposium on Computer-Based Medical Systems (IEEE \{CBMS\} 2008) -- Seven selected and extended papers on Biomedical Data Mining},
	Number = {12},
	Pages = {1452 - 1469},
	Title = {A survey on summarizability issues in multidimensional modeling},
	Url = {http://www.sciencedirect.com/science/article/pii/S0169023X09001104},
	Volume = {68},
	Year = {2009},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0169023X09001104},
	Bdsk-Url-2 = {http://dx.doi.org/10.1016/j.datak.2009.07.010}}

@article{Pedersen:2001,
	Abstract = {On-line analytical processing (OLAP) systems considerably improve data analysis and are finding wide-spread use. \{OLAP\} systems typically employ multidimensional data models to structure their data. This paper identifies 11 modeling requirements for multidimensional data models. These requirements are derived from an assessment of complex data found in real-world applications. A survey of 14 multidimensional data models reveals shortcomings in meeting some of the requirements. Existing models do not support many-to-many relationships between facts and dimensions, lack built-in mechanisms for handling change and time, lack support for imprecision, and are generally unable to insert data with varying granularities. This paper defines an extended multidimensional data model and algebraic query language that address all 11 requirements. The model reuses the common multidimensional concepts of dimension hierarchies and granularities to capture imprecise data. For queries that cannot be answered precisely due to the imprecise data, techniques are proposed that take into account the imprecision in the grouping of the data, in the subsequent aggregate computation, and in the presentation of the imprecise result to the user. In addition, alternative queries unaffected by imprecision are offered. The data model and query evaluation techniques discussed in this paper can be implemented using relational database technology. The approach is also capable of exploiting multidimensional query processing techniques like pre-aggregation. This yields a practical solution with low computational overhead. },
	Author = {Torben Bach Pedersen and Christian S. Jensen and Curtis E. Dyreson},
	Date-Added = {2014-09-15 13:48:20 +0000},
	Date-Modified = {2014-09-16 07:58:21 +0000},
	Doi = {10.1016/S0306-4379(01)00023-0},
	Issn = {0306-4379},
	Journal = {Information Systems},
	Keywords = {On-line analytical processing},
	Note = {Data Warehousing},
	Number = {5},
	Pages = {383 - 423},
	Title = {A foundation for capturing and querying complex multidimensional data},
	Url = {http://www.sciencedirect.com/science/article/pii/S0306437901000230},
	Volume = {26},
	Year = {2001},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0306437901000230},
	Bdsk-Url-2 = {http://dx.doi.org/10.1016/S0306-4379(01)00023-0}}

